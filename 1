using System;

class MainClass {
  public static void Main (string[] args) 
  {
    int N, i;
    int[] a = new int[10000];
    Console.Write("Введите число N: ");
    N = int.Parse(Console.ReadLine());
    for (i=1; i<=N; i++)
    {
      a[i]=i*2-1;
      Console.Write("a[");
      Console.Write(i);
      Console.Write("] = ");
      Console.Write(a[i]);
      Console.Write(", ");
    }
    Console.ReadKey();
  }
}
